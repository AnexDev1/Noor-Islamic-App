"translation":"34. \u12a5\u1290\u12da\u12eb\u121d \u12a5\u1290\u122d\u1231 \u1260\u1236\u120b\u1276\u127b\u1278\u12cd \u120b\u12ed \u12e8\u121a\u1320\u1263\u1260\u1241\u1275::","footnotes":""},{"id":"5410","sura":"70","aya":"35","arabic_text":"\u0623\u064f\u0648\u0652\u0644\u064e\u0670\u0653\u0626\u0650\u0643\u064e \u0641\u0650\u064a \u062c\u064e\u0646\u0651\u064e\u0670\u062a\u0656 \u0645\u0651\u064f\u0643\u06e1\u0631\u064e\u0645\u064f\u0648\u0646\u064e","translation":"35. \u12a5\u1290\u12da\u12eb (\u12a8\u12da\u1205 \u1260\u120b\u12ed \u12e8\u1270\u12c8\u1231\u1275 \u1201\u1209) \u1260\u1308\u1290\u1276\u127d \u12cd\u1235\u1325 \u12e8\u121a\u12a8\u1260\u1229 \u1293\u1278\u12cd::","footnotes":""},{"id":"5411","sura":"70","aya":"36","arabic_text":"\u0641\u064e\u0645\u064e\u0627\u0644\u0650 \u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0643\u064e\u0641\u064e\u0631\u064f\u0648\u0627\u0652 \u0642\u0650\u0628\u064e\u0644\u064e\u0643\u064e \u0645\u064f\u0647\u06e1\u0637\u0650\u0639\u0650\u064a\u0646\u064e","translation":"36. \u12a5\u1290\u12da\u12eb (\u1260\u12a0\u120b\u1205) \u12e8\u12ab\u12f1\u1275 \u12c8\u12f0 \u12a0\u1295\u1270 \u1260\u12a9\u120d \u12a0\u1295\u1308\u1276\u127b\u1278\u12cd\u1295 \u1218\u12d8\u12cd \u12e8\u121a\u12eb\u1270\u12a9\u1229\u1275 \u121d\u1295 \u12a0\u120b\u1278\u12cd?","footnotes":""},{"id":"5412","sura":"70","aya":"37","arabic_text":"\u0639\u064e\u0646\u0650 \u0671\u0644\u06e1\u064a\u064e\u0645\u0650\u064a\u0646\u0650 \u0648\u064e\u0639\u064e\u0646\u0650 \u0671\u0644\u0634\u0651\u0650\u0645\u064e\u0627\u0644\u0650 \u0639\u0650\u0632\u0650\u064a\u0646\u064e","translation":"37. \u12a8\u1240\u129d\u1293 \u12a8\u130d\u122b \u12ad\u134d\u120d\u134d\u120d \u1261\u12f5\u1296\u127d \u1232\u1206\u1291\u1364","footnotes":""},{"id":"5413","sura":"70","aya":"38","arabic_text":"\u0623\u064e\u064a\u064e\u0637\u06e1\u0645\u064e\u0639\u064f \u0643\u064f\u0644\u0651\u064f \u0671\u0645\u06e1\u0631\u0650\u064a\u0655\u0656 \u0645\u0651\u0650\u0646\u0