\u1235\u1228\u1302 \u12eb\u1218\u1323\u1201\u120b\u127d\u1201 \u1290\u129d\u1293 \u1260\u121b\u1208\u1275 \u1218\u1323\u120b\u1278\u12cd\u1362","footnotes":""},{"id":"4434","sura":"44","aya":"20","arabic_text":"\u0648\u064e\u0625\u0650\u0646\u0651\u0650\u064a \u0639\u064f\u0630\u06e1\u062a\u064f \u0628\u0650\u0631\u064e\u0628\u0651\u0650\u064a \u0648\u064e\u0631\u064e\u0628\u0651\u0650\u0643\u064f\u0645\u06e1 \u0623\u064e\u0646 \u062a\u064e\u0631\u06e1\u062c\u064f\u0645\u064f\u0648\u0646\u0650","translation":"20. \u00ab\u12a5\u1294 \u12a5\u1295\u12f3\u1275\u12c8\u130d\u1229\u129d \u1260\u130c\u1273\u12ec\u1293 \u1260\u130c\u1273\u127d\u1201 \u1270\u1320\u1265\u1242\u12eb\u1208\u1201\u1362","footnotes":""},{"id":"4435","sura":"44","aya":"21","arabic_text":"\u0648\u064e\u0625\u0650\u0646 \u0644\u0651\u064e\u0645\u06e1 \u062a\u064f\u0624\u06e1\u0645\u0650\u0646\u064f\u0648\u0627\u0652 \u0644\u0650\u064a \u0641\u064e\u0671\u0639\u06e1\u062a\u064e\u0632\u0650\u0644\u064f\u0648\u0646\u0650","translation":"21. \u00ab\u1260\u12a5\u1294\u121d \u1263\u1273\u121d\u1291 \u122b\u1241\u129d\u1362 \u1270\u12cd\u129d\u1362\u00bb \u12a0\u1208::","footnotes":""},{"id":"4436","sura":"44","aya":"22","arabic_text":"\u0641\u064e\u062f\u064e\u0639\u064e\u0627 \u0631\u064e\u0628\u0651\u064e\u0647\u064f\u06e5\u0653 \u0623\u064e\u0646\u0651\u064e \u0647\u064e\u0670\u0653\u0624\u064f\u0644\u064e\u0627\u0653\u0621\u0650 \u0642\u064e\u0648\u06e1\u0645\u065e \u0645\u0651\u064f\u062c\u06e1\u0631\u0650\u0645\u064f\u0648\u0646\u064e","translation":"22. \u00ab\u1240\u1325\u120e\u121d \u12a5\u1290\u12da\u1205 \u12a0\u1218\u1338\u129e\u127d \u1205\u12dd\u1266\u127d \u1293\u1278\u12cd (\u12a0\u1325\u134b\u1278\u12cd)\u1362\u00bb \u1232\u120d \u130c\u1273\u12cd\u1295 \u1208\u1218\u1290\u1362","footnotes":""},{"id":"4437","sura":"44","aya":"23","arabic_text":"\u0641\u064e\u0623\u064e\u0633\u06e1\u0631\u0650 \u0628\u0650\u0639\u0650\u0628\u064e\u0627\u062f\u0650\u064a \u0644\u064e\u064a\u06e1\u0644\u064b\u0627 \u0625\u0650\u0646\u0651\u064e\u0643\u064f\u0645 \u0645\u0651\u064f\u062a\u0651\u064e\u0628\u064e\u0639\u064f\u0648\u0646\u064e","translation":"23. (\u130c\u1273\u12c9\u121d \u12a0\u1208) \u00ab\u1263\u122e\u127c\u1295\u121d \u12ed\u12d8\u1205 \u1260\u120c\u120a\u1275 \u1202\u12f5\u1362 \u12a5\u1293\u1295\u1270 \u12e8\u121a\u12a8\u1270\u1209\u12cb\u127d\u1201 \u1293\u127d\u1201\u1293\u1362","footnotes":""},{"id":"4438","sura":"44","aya":"24","arabic_text":"\u0648\u064e\u0671\u062a\u06e1\u0631\u064f\u0643\u0650 \u0671\u0644\u06e1\u0628\u064e\u062d\u06e1\u0631\u064e \u0631\u064e\u0647\u06e1\u0648\u064b\u0627\u06d6 \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u062c\u064f\u0646\u062f\u065e \u0645\u0651\u064f\u063a\u06e1\u0631\u064e\u0642\u064f\u0648\u0646\u064e","translation":"24. \u00ab\u1263\u1205\u1229\u1295\u121d \u12e8\u1270\u12a8\u1348\u1270 \u1206\u1296 \u1270\u12c8\u12cd\u1364 \u12a5\u1290\u122d\u1231 \u12e8\u121a\u1230\u121d\u1321 \u1230\u122b\u12ca\u1275 \u1293\u1278\u12cd\u1293\u00bb \u12a0\u1208\u12cd\u1362","footnotes":""},{"id":"4439","sura":"44","aya":"25","arabic_text":"\u0643\u064e\u0645\u06e1 \u062a\u064e\u0631\u064e\u0643\u064f\u0648\u0627\u0652 \u0645\u0650\u0646 \u062c\u064e\u0646\u0651\u064e\u0670\u062a\u0656 \u0648\u064e\u0639\u064f\u064a\u064f\u0648\u0646\u0656","translation":"25. \u12a8\u12a0\u1275\u12ad\u120d\u1276\u127d\u1293 \u12a8\u121d\u1295\u132e\u127d \u1265\u12d9 \u1290\u1308\u122e\u127d\u1295 \u1275\u1270\u12cd \u1204\u12f1\u1362","footnotes":""},{"id":"4440","sura":"44","aya":"26","arabic_text":"\u0648\u064e\u0632\u064f\u0631\u064f\u0648\u0639\u0656 \u0648\u064e\u0645\u064e\u0642\u064e\u0627\u0645\u0656 \u0643\u064e\u0631\u0650\u064a\u0645\u0656","translation":"26. \u12a8\u12a0\u12dd\u1218\u122b\u12ce\u127d\u121d \u12a8\u1218\u120d\u12ab\u121d \u1218\u1240\u1218\u132b\u121d","footnotes":""},{"id":"4441","sura":"44","aya":"27","arabic_text":"\u0648\u064e\u0646\u064e\u0639\u06e1\u0645\u064e\u0629\u0656 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u0641\u0650\u064a\u0647\u064e\u0627 \u0641\u064e\u0670\u0643\u0650\u0647\u0650\u064a\u0646\u064e","translation":"27. \u1260\u12a5\u122d\u1237 \u1270\u12f0\u1233\u127e\u127d \u12a8\u1290\u1260\u1229\u1263\u1275\u121d \u12f5\u120e\u1275 (\u1265\u12d9\u1295 \u1290\u1308\u122d \u1275\u1270\u12cd \u121e\u1271)\u1362","footnotes":""},{"id":"4442","sura":"44","aya":"28","arabic_text":"\u0643\u064e\u0630\u064e\u0670\u0644\u0650\u0643\u064e\u06d6 \u0648\u064e\u0623\u064e\u0648\u06e1\u0631\u064e\u062b\u06e1\u0646\u064e\u0670\u0647\u064e\u0627 \u0642\u064e\u0648\u06e1\u0645\u064b\u0627 \u0621\u064e\u0627\u062e\u064e\u0631\u0650\u064a\u0646\u064e","translation":"28. \u1290\u1308\u1229 \u120d\u12ad \u12a5\u1295\u12f0\u12da\u1205 \u1206\u1290\u1364 \u1208\u120c\u120e\u127d \u1205\u12dd\u1266\u127d \u12a0\u12c8\u1228\u1235\u1293\u1275\u121d\u1362","footnotes":""},{"id":"4443","sura":"44","aya":"29","arabic_text":"\u0641\u064e\u0645\u064e\u0627 \u0628\u064e\u0643\u064e\u062a\u06e1 \u0639\u064e\u0644\u064e\u064a\u06e1\u0647\u0650\u0645\u064f \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0627\u0653\u0621\u064f \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064f \u0648\u064e\u0645\u064e\u0627 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u0645\u064f\u0646\u0638\u064e\u0631\u0650\u064a\u0646\u064e","translation":"29. \u1230\u121b\u12ed\u1293 \u121d\u12f5\u122d \u1260\u12a5\u1290\u122d\u1231 \u120b\u12ed \u12a0\u120b\u1208\u1240\u1231\u121d:: \u12e8\u121a\u1246\u12e9\u121d \u12a0\u120d\u1290\u1260\u1229\u121d::","footnotes":""},{"id":"4444","sura":"44","aya":"30","arabic_text":"\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u06e1 \u0646\u064e\u062c\u0651\u064e\u064a\u06e1\u0646\u064e\u0627 \u0628\u064e\u0646\u0650\u064a\u0653 \u0625\u0650\u0633\u06e1\u0631\u064e\u0670\u0653\u0621\u0650\u064a\u0644\u064e \u0645\u0650\u0646\u064e \u0671\u0644\u06e1\u0639\u064e\u0630\u064e\u0627\u0628\u0650 \u0671\u0644\u06e1\u0645\u064f\u0647\u0650\u064a\u0646\u0650","translation":"30. \u12e8\u12a2\u1235\u122b\u12a2\u120d \u120d\u1306\u127d\u1295 \u12a8\u12da\u12eb \u12a0\u12cb\u122b\u1305 \u12a8\u1206\u1290\u12cd \u1235\u1243\u12ed \u1260\u12a5\u122d\u130d\u1325 \u12a0\u12f3\u1295\u1293\u1278\u12cd::","footnotes":""},{"id":"4445","sura":"44","aya":"31","arabic_text":"\u0645\u0650\u0646 \u0641\u0650\u0631\u06e1\u0639\u064e\u0648\u06e1\u0646\u064e\u06da \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0643\u064e\u0627\u0646\u064e \u0639\u064e\u0627\u0644\u0650\u064a\u0657\u0627 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u06e1\u0645\u064f\u0633\u06e1\u0631\u0650\u0641\u0650\u064a\u0646\u064e","translation":"31. \u12a8\u1348\u122d\u12d6\u1295 \u12a5\u122d\u1231 (\u1348\u122d\u12d6\u12cd\u1295) \u12e8\u12ae\u122b \u12a8\u12c8\u1230\u1295 \u12a0\u120b\u134a\u12ce\u127d\u121d \u12a0\u1295\u12f1 \u1290\u1260\u122d\u1293::","footnotes":""},{"id":"4446","sura":"44","aya":"32","arabic_text":"\u0648\u064e\u0644\u064e\u0642\u064e\u062f\u0650 \u0671\u062e\u06e1\u062a\u064e\u0631\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1 \u0639\u064e\u0644\u064e\u0649\u0670 \u0639\u0650\u0644\u06e1\u0645\u064d \u0639\u064e\u0644\u064e\u0649 \u0671\u0644\u06e1\u0639\u064e\u0670\u0644\u064e\u0645\u0650\u064a\u0646\u064e","translation":"32. \u12a8\u121b\u12c8\u1245\u121d \u130b\u122d \u12a8\u12d3\u1208\u121d \u1205\u12dd\u1266\u127d \u1218\u12ab\u12a8\u120d \u1260\u12a5\u122d\u130d\u1325 \u1218\u1228\u1325\u1293\u1278\u12cd::","footnotes":""},{"id":"4447","sura":"44","aya":"33","arabic_text":"\u0648\u064e\u0621\u064e\u0627\u062a\u064e\u064a\u06e1\u0646\u064e\u0670\u0647\u064f\u0645 \u0645\u0651\u0650\u0646\u064e \u0671\u0644\u06e1\u0623\u0653\u064a\u064e\u0670\u062a\u0650 \u0645\u064e\u0627 \u0641\u0650\u064a\u0647\u0650 \u0628\u064e\u0644\u064e\u0670\u0653\u0624\u065e\u0627\u0652 \u0645\u0651\u064f\u0628\u0650\u064a\u0646\u064c","translation":"33. \u12a8\u1270\u12a0\u121d\u122b\u1276\u127d\u121d \u1260\u12cd\u1235\u1321 \u130d\u120d\u133d \u12e8\u1206\u1290 \u1348\u1270\u1293 \u12eb\u1208\u1260\u1275\u1295 \u1230\u1320\u1293\u1278\u12cd::","footnotes":""},{"id":"4448","sura":"44","aya":"34","arabic_text":"\u0625\u0650\u0646\u0651\u064e \u0647\u064e\u0670\u0653\u0624\u064f\u0644\u064e\u0627\u0653\u0621\u0650 \u0644\u064e\u064a\u064e\u0642\u064f\u0648\u0644\u064f\u0648\u0646\u064e","translation":"34. \u12a5\u1290\u12da\u1205 (\u12e8\u1218\u12ab \u12ab\u1213\u12f2\u12eb\u1295) \u1260\u12a5\u122d\u130d\u1325 \u12ed\u120b\u1209:","footnotes":""},{"id":"4449","sura":"44","aya":"35","arabic_text":"\u0625\u0650\u0646\u06e1 \u0647\u0650\u064a\u064e \u0625\u0650\u0644\u0651\u064e\u0627 \u0645\u064e\u0648\u06e1\u062a\u064e\u062a\u064f\u0646\u064e\u0627 \u0671\u0644\u06e1\u0623\u064f\u0648\u0644\u064e\u0649\u0670 \u0648\u064e\u0645\u064e\u0627 \u0646\u064e\u062d\u06e1\u0646\u064f \u0628\u0650\u0645\u064f\u0646\u0634\u064e\u0631\u0650\u064a\u0646\u064e","translation":"35. \u00ab\u12a5\u122d\u1237 \u12e8\u134a\u1270\u129b\u12ed\u1271 \u121e\u1273\u127d\u1295 \u12a5\u1295\u1305 (\u120c\u120b) \u12a0\u12ed\u12f0\u1208\u127d\u121d\u1362 \u12a5\u129b\u121d \u12e8\u121d\u1295\u1240\u1230\u1240\u1235 \u12a0\u12ed\u12f0\u1208\u1295\u121d\u1362","footnotes":""},{"id":"4450","sura":"44","aya":"36","arabic_text":"\u0641\u064e\u0623\u06e1\u062a\u064f\u0648\u0627\u0652 \u0628\u0650\u0640\u0654\u064e\u0627\u0628\u064e\u0627\u0653\u0626\u0650\u0646\u064e\u0627\u0653 \u0625\u0650\u0646 \u0643\u064f\u0646\u062a\u064f\u0645\u06e1 \u0635\u064e\u0670\u062f\u0650\u0642\u0650\u064a\u0646\u064e","translation":"36. \u00ab\u12a5\u12cd\u1290\u1270\u129e\u127d\u121d \u12a5\u1295\u12f0\u1206\u1293\u127d\u1201 \u12a0\u1263\u1276\u127b\u127d\u1295\u1295 \u12a5\u1235\u1272 \u12a0\u121d\u1321\u120d\u1295\u1362\u00bb (\u12ed\u120b\u1209)","footnotes":""},{"id":"4451","sura":"44","aya":"37","arabic_text":"\u0623\u064e\u0647\u064f\u0645\u06e1 \u062e\u064e\u064a\u06e1\u0631\u064c \u0623\u064e\u0645\u06e1 \u0642\u064e\u0648\u06e1\u0645\u064f \u062a\u064f\u0628\u0651\u064e\u0639\u0656 \u0648\u064e\u0671\u0644\u0651\u064e\u0630\u0650\u064a\u0646\u064e \u0645\u0650\u0646 \u0642\u064e\u0628\u06e1\u0644\u0650\u0647\u0650\u0645\u06e1 \u0623\u064e\u0647\u06e1\u0644\u064e\u0643\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u06e1\u06da \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u0645\u06e1 \u0643\u064e\u0627\u0646\u064f\u0648\u0627\u0652 \u0645\u064f\u062c\u06e1\u0631\u0650\u0645\u0650\u064a\u0646\u064e","translation":"37. \u12a5\u1290\u122d\u1231 \u1260\u120b\u132e\u127d \u1293\u1278\u12cd\u1295? \u12c8\u12ed\u1235 \u12e8\u1271\u1260\u12d5 \u1205\u12dd\u1266\u127d\u1293 \u12a5\u1290\u12da\u12eb \u12a8\u12a5\u1290\u122d\u1231 \u1260\u134a\u1275 \u12e8\u1290\u1260\u1229\u1275? \u12a5\u1290\u122d\u1231\u1295 \u12a0\u1320\u134b\u1293\u1278\u12cd\u1362 \u12a5\u1290\u122d\u1231 \u12a0\u1218\u1338\u129e\u127d \u1290\u1260\u1229\u1362","footnotes":""},{"id":"4452","sura":"44","aya":"38","arabic_text":"\u0648\u064e\u0645\u064e\u0627 \u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0627 \u0671\u0644\u0633\u0651\u064e\u0645\u064e\u0670\u0648\u064e\u0670\u062a\u0650 \u0648\u064e\u0671\u0644\u06e1\u0623\u064e\u0631\u06e1\u0636\u064e \u0648\u064e\u0645\u064e\u0627 \u0628\u064e\u064a\u06e1\u0646\u064e\u0647\u064f\u0645\u064e\u0627 \u0644\u064e\u0670\u0639\u0650\u0628\u0650\u064a\u0646\u064e","translation":"38. \u1230\u121b\u12eb\u1275\u1293 \u121d\u12f5\u122d\u1295 \u1260\u1201\u1208\u1271 \u1218\u12ab\u12a8\u120d \u12eb\u1208\u12cd\u1295\u121d \u1201\u1209 \u1240\u120b\u1306\u127d \u1206\u1290\u1295 \u12a0\u120d\u1348\u1320\u122d\u1295\u121d::","footnotes":""},{"id":"4453","sura":"44","aya":"39","arabic_text":"\u0645\u064e\u0627 \u062e\u064e\u0644\u064e\u0642\u06e1\u0646\u064e\u0670\u0647\u064f\u0645\u064e\u0627\u0653 \u0625\u0650\u0644\u0651\u064e\u0627 \u0628\u0650\u0671\u0644\u06e1\u062d\u064e\u0642\u0651\u0650 \u0648\u064e\u0644\u064e\u0670\u0643\u0650\u0646\u0651\u064e \u0623\u064e\u0643\u06e1\u062b\u064e\u0631\u064e\u0647\u064f\u0645\u06e1 \u0644\u064e\u0627 \u064a\u064e\u0639\u06e1\u0644\u064e\u0645\u064f\u0648\u0646\u064e","translation":"39. \u1201\u1208\u1271\u1295\u121d \u1260\u1241\u121d\u1290\u1308\u122d \u12a5\u1295\u1302 \u12a0\u120d\u1348\u1320\u122d\u1293\u1278\u12c9\u121d:: \u130d\u1295 \u12a0\u1265\u12db\u1200\u129e\u127b\u1278\u12cd \u12a0\u12eb\u12cd\u1240\u12c9\u121d::","footnotes":""},{"id":"4454","sura":"44","aya":"40","arabic_text":"\u0625\u0650\u0646\u0651\u064e \u064a\u064e\u0648\u06e1\u0645\u064e \u0671\u0644\u06e1\u0641\u064e\u0635\u06e1\u0644\u0650 \u0645\u0650\u064a\u0642\u064e\u0670\u062a\u064f\u0647\u064f\u0645\u06e1 \u0623\u064e\u062c\u06e1\u0645\u064e\u0639\u0650\u064a\u0646\u064e","translation":"40. \u12e8\u1218\u1208\u12eb\u12cd \u1240\u1295 \u1208\u1201\u1209\u121d \u1240\u1320\u122f\u1278\u12cd \u1290\u12cd::","footnotes":""},{"id":"4455","sura":"44","aya":"41","arabic_text":"\u064a\u064e\u0648\u06e1\u0645\u064e \u0644\u064e\u0627 \u064a\u064f\u063a\u06e1\u0646\u0650\u064a \u0645\u064e\u0648\u06e1\u0644\u064b\u0649 \u0639\u064e\u0646 \u0645\u0651\u064e\u0648\u06e1\u0644\u0657\u0649 \u0634\u064e\u064a\u06e1\u0640\u0654\u0657\u0627 \u0648\u064e\u0644\u064e\u0627 \u0647\u064f\u0645\u06e1 \u064a\u064f\u0646\u0635\u064e\u0631\u064f\u0648\u0646\u064e","translation":"41. \u12d8\u1218\u12f5 \u12a8\u12d8\u1218\u12f1 \u121d\u1295\u1295\u121d \u12e8\u121b\u12ed\u1320\u1245\u121d\u1260\u1275 \u12a5\u1290\u122d\u1231\u121d \u12e8\u121b\u12ed\u1228\u12f1\u1260\u1275 \u1240\u1295 \u1290\u12cd::","footnotes":""},{"id":"4456","sura":"44","aya":"42","arabic_text":"\u0625\u0650\u0644\u0651\u064e\u0627 \u0645\u064e\u0646 \u0631\u0651\u064e\u062d\u0650\u0645\u064e \u0671\u0644\u0644\u0651\u064e\u0647\u064f\u06da \u0625\u0650\u0646\u0651\u064e\u0647\u064f\u06e5 \u0647\u064f\u0648\u064e \u0671\u0644\u06e1\u0639\u064e\u0632\u0650\u064a\u0632\u064f \u0671\u0644\u0631\u0651\u064e\u062d\u0650\u064a\u0645\u064f","translation":"42. \u12a0\u120b\u1205 \u12eb\u12d8\u1290\u1208\u1275\u1293 \u12e